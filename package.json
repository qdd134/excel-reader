{
  "name": "excel-reader",
  "version": "1.0.0",
  "description": "A TypeScript library for reading Excel files with embedded images and converting them to base64",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node example/simple-example.js",
    "example": "npm run build && node example/simple-example.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "excel",
    "xlsx",
    "image",
    "base64",
    "typescript",
    "sheetjs"
  ],
  "author": "Qdd",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.3.6",
    "jszip": "^3.10.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "dotenv": "^17.2.2",
    "typescript": "^5.0.0"
  }
}
